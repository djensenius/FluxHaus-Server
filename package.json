{
  "name": "fluxhaus-server",
  "version": "1.2.4",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "webpack --node-env=production --mode=production",
    "watch": "webpack --watch",
    "test": "npx jest",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "test-watch": "npx jest --watchAll --coverage",
    "build-watch": "npx webpack -w --mode=production",
    "build-production": "npx webpack --mode=production",
    "build-development": "npx webpack --mode=development",
    "build-analyze": "npx webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "login-homeconnect": "npx ts-node scripts/login-homeconnect.ts",
    "login-miele": "npx ts-node scripts/login-miele.ts",
    "server": "npx nodemon --ignore cache/ ./dist/server.js"
  },
  "keywords": [],
  "author": "David Jensenius <david@jensenius.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "bluelinky": "^10.0.0",
    "cors": "^2.8.5",
    "dorita980": "npm:@karlvr/dorita980@3.2.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^8.2.1",
    "fetch-sse": "^1.1.3",
    "nocache": "^4.0.0"
  }
}
